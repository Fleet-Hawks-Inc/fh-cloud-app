{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "fleethawks-dashboard": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/fleethawks-dashboard",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot":false,
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "src/assets/css/jquerytable.css",
              "src/styles.css",
              "src/assets/vendor/select2-bootstrap-theme/select2-bootstrap.min.css",
              "node_modules/ngx-toastr/toastr.css",
              "node_modules/jstree/dist/themes/default/style.min.css",
              "node_modules/slick-carousel/slick/slick.scss",
              "node_modules/slick-carousel/slick/slick-theme.scss",
              "node_modules/select2/dist/css/select2.min.css"
            ],
            "scripts": [
              "src/assets/vendor/jquery/jquery.js",
              "src/assets/vendor/jquery-browser-mobile/jquery.browser.mobile.js",
              "node_modules/jszip/dist/jszip.js",
              "src/assets/vendor/modernizr/modernizr.min.js",
              "src/assets/vendor/popper/umd/popper.min.js",
              "src/assets/vendor/bootstrap/js/bootstrap.js",
              "src/assets/vendor/bootstrap-datepicker/js/bootstrap-datepicker.js",
              "src/assets/vendor/common/common.js",
              "src/assets/vendor/jquery-placeholder/jquery.placeholder.js",
              "src/assets/js/theme.js",
              "node_modules/jstree/dist/jstree.min.js",
              "src/assets/vendor/nanoscroller/nanoscroller.js",
              "src/assets/js/custom.js",
              "src/assets/js/theme.init.js",
              "src/assets/js/examples/examples.dashboard.js",
              "node_modules/chart.js/dist/Chart.bundle.min.js",
              "node_modules/slick-carousel/slick/slick.min.js",
              "src/assets/js/jquery.validate.min.js",
              "node_modules/select2/dist/js/select2.min.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": true,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                 "maximumWarning": "22mb",
                  "maximumError": "22mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "22mb",
                  "maximumError": "22mb"
                }
              ]
            },
            "preprod": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.preprod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": true,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "22mb",
                  "maximumError": "22mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "22mb",
                  "maximumError": "22mb"
                }
              ]
            },
            "dev": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.dev.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": true,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "22mb",
                  "maximumError": "22mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "22mb",
                  "maximumError": "22mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "fleethawks-dashboard:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "fleethawks-dashboard:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "fleethawks-dashboard:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "fleethawks-dashboard:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "fleethawks-dashboard:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "fleethawks-dashboard",
  "cli": {
    "analytics": "acb93201-6c0f-42b0-b193-a2570c85821e"
  }
}
