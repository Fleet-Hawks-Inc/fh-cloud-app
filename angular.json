{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "fleethawks-dashboard": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/fleethawks-dashboard",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": false,
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "node_modules/datatables.net-dt/css/jquery.dataTables.css",
              "node_modules/mapbox-gl/dist/mapbox-gl.css",
              "node_modules/@mapbox/mapbox-gl-directions/dist/mapbox-gl-directions.css",
              "node_modules/@mapbox/mapbox-gl-draw/dist/mapbox-gl-draw.css",
              "node_modules/@mapbox/mapbox-gl-geocoder/dist/mapbox-gl-geocoder.css",
              "src/styles.css"
            ],
            "scripts": [
              "node_modules/mapbox-gl/dist/mapbox-gl.js",
              "node_modules/@mapbox/mapbox-gl-directions/dist/mapbox-gl-directions.js",
              "src/assets/vendor/modernizr/modernizr.min.js",
              "src/assets/vendor/jquery/jquery.js",
              "src/assets/vendor/jquery-browser-mobile/jquery.browser.mobile.js",
              "src/assets/vendor/popper/umd/popper.min.js",
              "src/assets/vendor/bootstrap/js/bootstrap.js",
              "src/assets/vendor/bootstrap-datepicker/js/bootstrap-datepicker.js",
              "src/assets/vendor/common/common.js",
              "src/assets/vendor/nanoscroller/nanoscroller.js",
              "src/assets/vendor/magnific-popup/jquery.magnific-popup.js",
              "src/assets/vendor/jquery-placeholder/jquery.placeholder.js",
              "src/assets/js/theme.js",
              "src/assets/js/custom.js",
              "src/assets/js/theme.init.js",
              "src/assets/js/examples/examples.dashboard.js",
              "node_modules/datatables.net/js/jquery.dataTables.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "fleethawks-dashboard:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "fleethawks-dashboard:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "fleethawks-dashboard:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "fleethawks-dashboard:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "fleethawks-dashboard:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "fleethawks-dashboard"
}