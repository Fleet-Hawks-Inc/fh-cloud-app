{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "cli": {
        "analytics": "cd69feba-6fc4-4dd2-af5b-0cd13a13738b"
    },
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "fleethawks-dashboard": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss"
                }
            },
            "root": "",
            "sourceRoot": "src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/fleethawks-dashboard",
                        "index": "src/index.html",
                        "main": "src/main.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "tsconfig.app.json",
                        "aot": false,
                        "assets": [
                            "src/favicon.ico",
                            "src/assets",
                            "src/OneSignalSDKWorker.js"
                        ],
                        "vendorChunk": false,
                        "extractLicenses": false,
                        "buildOptimizer": false,
                        "sourceMap": false,
                        "optimization": false,
                        "namedChunks": false,
                        "styles": [
                            "src/styles.scss",
                            "node_modules/select2/dist/css/select2.min.css",
                            "node_modules/@swimlane/ngx-datatable/index.css",
                        ],
                        "scripts": [
                            "node_modules/jszip/dist/jszip.js",
                            "src/assets/vendor/modernizr/modernizr.min.js",
                            "src/assets/vendor/bootstrap/js/bootstrap.js",
                            "src/assets/vendor/bootstrap-datepicker/js/bootstrap-datepicker.js",
                            "src/assets/vendor/common/common.js",
                            "src/assets/vendor/jquery-placeholder/jquery.placeholder.js"
                        ]
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.prod.ts"
                                }
                            ],
                            "aot": true,
                            "optimization": {
                                "scripts": true,
                                "styles": {
                                    "minify": true,
                                    "inlineCritical": false
                                },
                                "fonts": true
                            },
                            "outputHashing": "all",
                            "sourceMap": false,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "22mb",
                                    "maximumError": "22mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "22mb",
                                    "maximumError": "22mb"
                                }
                            ]
                        },
                        "preprod": {
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.preprod.ts"
                                }
                            ],
                            "aot": true,
                            "optimization": {
                                "scripts": true,
                                "styles": {
                                    "minify": true,
                                    "inlineCritical": false
                                },
                                "fonts": true
                            },
                            "outputHashing": "all",
                            "sourceMap": false,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "22mb",
                                    "maximumError": "22mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "22mb",
                                    "maximumError": "22mb"
                                }
                            ]
                        },
                        "dev": {
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.dev.ts"
                                }
                            ],
                            "aot": true,
                            "optimization": {
                                "scripts": true,
                                "styles": {
                                    "minify": true,
                                    "inlineCritical": false
                                },
                                "fonts": true
                            },
                            "outputHashing": "all",
                            "sourceMap": false,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "22mb",
                                    "maximumError": "22mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "22mb",
                                    "maximumError": "22mb"
                                }
                            ]
                        }
                    },
                    "defaultConfiguration": ""
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "fleethawks-dashboard:build"
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "fleethawks-dashboard:build:production"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "fleethawks-dashboard:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "src/test.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "tsconfig.spec.json",
                        "karmaConfig": "karma.conf.js",
                        "assets": [
                            "src/favicon.ico",
                            "src/assets"
                        ],
                        "styles": [
                            "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
                            "src/styles.css"
                        ],
                        "scripts": []
                    }
                },
                "e2e": {
                    "builder": "@angular-devkit/build-angular:protractor",
                    "options": {
                        "protractorConfig": "e2e/protractor.conf.js",
                        "devServerTarget": "fleethawks-dashboard:serve"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "fleethawks-dashboard:serve:production"
                        }
                    }
                }
            }
        }
    }
}